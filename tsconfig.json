{
	"compilerOptions": {
		"baseUrl": "./src/index.ts",
		"lib": [
			"dom",
			"es2020"
		],
		"outDir": "lib",
		"target": "es2020",
		"module": "es2020",
		"moduleResolution": "node",
		"declaration": true,
		"noEmit": false,
		"noImplicitAny": true,
		"strict": false,
		"typeRoots": [
			"./node_modules/@types",
		],
		"paths": {}
	},
	"exclude": [
		"./node_modules/*",
		"./lib/*"
	]
}